{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "rota": {
      "type": "object",
      "properties": {
        "codigoBanco": { "type": "integer" },
        "codigoUnimed": { "type": "integer" },
        "numeroFatura": { "type": "integer" },
        "tipo": { "type": "string" }
      },
      "required": ["codigoBanco", "codigoUnimed", "numeroFatura", "tipo"],
      "additionalProperties": false
    },
    "dadosBoleto": {
      "type": "object",
      "properties": {
        "numeroDocumento": { "type": "string" },
        "dataVencimento": { "type": "string", "format": "date" },
        "dataEmissao": { "type": "string", "format": "date" },
        "tipoJuros": { "type": "integer" },
        "tipoMulta": { "type": "integer" },
        "dataMulta": { "type": "string", "format": "date" },
        "valorNominal": { "type": "number" },
        "valorJuros": { "type": "number" },
        "percentualJuros": { "type": "number" },
        "percentualMulta": { "type": "number" },
        "numeroNossoNumero": { "type": "string" },
        "linhaDigitavel": { "type": "string" },
        "codigoBarras": { "type": "string" },
        "especieDocumento": { "type": "string" },
        "aceite": { "type": "string" },
        "dataProcessamento": { "type": "string", "format": "date" },
        "quantidadeDiasProtesto": { "type": "integer" },
        "quantidadeDiasNegativacao": { "type": "integer" },
        "orgaoNegativador": { "type": ["string", "null"] },
        "aceiteTituloVencido": { "type": "string" },
        "numeroDiasLimiteRecebimento": { "type": "integer" },
        "mensagemBloquetoOcorrencia": { "type": ["string", "null"] },
        "codigoTipoTitulo": { "type": "integer" }
      },
      "required": [
        "numeroDocumento",
        "dataVencimento",
        "dataEmissao",
        "tipoJuros",
        "tipoMulta",
        "dataMulta",
        "valorNominal",
        "valorJuros",
        "percentualJuros",
        "percentualMulta",
        "numeroNossoNumero",
        "linhaDigitavel",
        "codigoBarras",
        "especieDocumento",
        "aceite",
        "dataProcessamento",
        "quantidadeDiasProtesto",
        "quantidadeDiasNegativacao",
        "orgaoNegativador",
        "aceiteTituloVencido",
        "numeroDiasLimiteRecebimento",
        "mensagemBloquetoOcorrencia",
        "codigoTipoTitulo"
      ],
      "additionalProperties": false
    },
    "dadosBanco": {
      "type": "object",
      "properties": {
        "codigoBanco": { "type": "integer" },
        "nomeBanco": { "type": "string" },
        "agencia": { "type": "string" },
        "digitoAgencia": { "type": "string" },
        "conta": { "type": "string" },
        "digitoConta": { "type": "string" },
        "convenio": { "type": "string" },
        "carteira": { "type": "string" },
        "variacao": { "type": "integer" },
        "codigoModalidade": { "type": "integer" }
      },
      "required": [
        "codigoBanco",
        "nomeBanco",
        "agencia",
        "digitoAgencia",
        "conta",
        "digitoConta",
        "convenio",
        "carteira",
        "variacao",
        "codigoModalidade"
      ],
      "additionalProperties": false
    },
    "beneficiario": {
      "type": "object",
      "properties": {
        "nome": { "type": "string" },
        "documento": { "type": "string" },
        "tipoPessoa": { "type": "string" },
        "tipoPessoaNumero": { "type": "integer" },
        "endereco": {
          "type": "object",
          "properties": {
            "logradouro": { "type": "string" },
            "cidade": { "type": "string" },
            "uf": { "type": "string" },
            "cep": { "type": "string" }
          },
          "required": ["logradouro", "cidade", "uf", "cep"],
          "additionalProperties": false
        },
        "telefone": { "type": "string" }
      },
      "required": ["nome", "documento", "tipoPessoa", "tipoPessoaNumero", "endereco", "telefone"],
      "additionalProperties": false
    },
    "pagador": {
      "type": "object",
      "properties": {
        "nome": { "type": "string" },
        "documento": { "type": "string" },
        "tipoPessoaNumero": { "type": "integer" },
        "tipoPessoa": { "type": "string" },
        "endereco": {
          "type": "object",
          "properties": {
            "logradouro": { "type": "string" },
            "bairro": { "type": "string" },
            "cidade": { "type": "string" },
            "uf": { "type": "string" },
            "cep": { "type": "string" },
            "complemento": { "type": ["string", "null"] }
          },
          "required": ["logradouro", "bairro", "cidade", "uf", "cep", "complemento"],
          "additionalProperties": false
        },
        "telefone": { "type": "string" }
      },
      "required": ["nome", "documento", "tipoPessoaNumero", "tipoPessoa", "endereco", "telefone"],
      "additionalProperties": false
    },
    "instrucoes": {
      "type": "array",
      "items": { "type": "string" }
    },
    "mensagens": {
      "type": "array",
      "items": { "type": "string" }
    },
    "descontos": {
      "type": "object",
      "properties": {
        "valorDesconto": { "type": "number" },
        "percentualDesconto": { "type": "number" }
      },
      "required": ["valorDesconto", "percentualDesconto"],
      "additionalProperties": false
    },
    "controlesInternos": {
      "type": "object",
      "properties": {
        "numeroFatura": { "type": "string" },
        "codigoUnimed": { "type": "string" },
        "contratoVenda": { "type": "string" },
        "tipoContrato": { "type": ["string", "null"] },
        "numeroProcesso": { "type": ["string", "null"] },
        "competencia": { "type": ["string", "null"] }
      },
      "required": ["numeroFatura", "codigoUnimed", "contratoVenda", "tipoContrato", "numeroProcesso", "competencia"],
      "additionalProperties": false
    }
  },
  "required": [
    "rota",
    "dadosBoleto",
    "dadosBanco",
    "beneficiario",
    "pagador",
    "instrucoes",
    "mensagens",
    "descontos",
    "controlesInternos"
  ],
  "additionalProperties": false
}
